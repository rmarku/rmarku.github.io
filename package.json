    "@mdx-js/react": "^1.5.7",
    "bootstrap": "^4.4.1",
    "code-surfer": "^3.1.0",
    "gatsby": "^2.19.25",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-mdx": "^1.0.75",
    "gatsby-plugin-sass": "^2.1.29",
